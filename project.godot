; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="IndieBlueprint"
config/description="Indie Blueprint is a comprehensive Godot project template designed to streamline your development process. It includes essential features, optimized settings, and best practices to help you create amazing indie game"
config/version="1.3.0"
run/main_scene="res://scenes/entry_point/game-entry_point.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

IndieBlueprintGamepadControllerManager="*res://autoload/gamepad/gamepad_controller_manager.gd"
IndieBlueprintGameGlobals="*res://autoload/general/game_globals.gd"
IndieBlueprintGlobalGameEvents="*res://autoload/general/global_game_events.gd"
IndieBlueprintCursorManager="*res://autoload/screen/cursor_manager.gd"
IndieBlueprintGlobalEffects="*res://autoload/effects/global_effects.tscn"
IndieBlueprintSettingsManager="*res://autoload/persistence/settings/settings_manager.gd"
IndieBlueprintAudioManager="*res://addons/ninetailsrabbit.indie_blueprint_audio/src/audio_manager.gd"
IndieBlueprintMusicManager="*res://addons/ninetailsrabbit.indie_blueprint_audio/src/music_manager.gd"
IndieBlueprintSoundPool="*res://addons/ninetailsrabbit.indie_blueprint_audio/src/sound_pool.gd"
IndieBlueprintCameraTransitioner="*res://addons/ninetailsrabbit.indie_blueprint_camera_transitioner/src/camera_transitioner.tscn"
IndieBlueprintGlobalClock="*res://addons/ninetailsrabbit.indie_blueprint_global_clock/src/global_clock.tscn"
IndieBlueprintLootManager="*res://addons/ninetailsrabbit.indie_blueprint_rpg/src/probability/loot/loot_manager.gd"
IndieBlueprintTurnityManager="*res://addons/ninetailsrabbit.indie_blueprint_rpg/src/turns/turnity_manager.gd"
IndieBlueprintObjectPoolManager="*res://addons/ninetailsrabbit.indie_blueprint_pool/src/object_pool_manager.gd"
IndieBlueprintWindowManager="*res://addons/ninetailsrabbit.indie_blueprint_toolbox/src/autoloads/viewport/window_manager.gd"
IndieBlueprintSceneTransitioner="*res://addons/ninetailsrabbit.indie_blueprint_scene_transition/src/scene_transitioner.tscn"
IndieBlueprintSaveManager="*res://addons/ninetailsrabbit.indie_blueprint_save/src/save_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720

[editor_plugins]

enabled=PackedStringArray("res://addons/ninetailsrabbit.indie_blueprint_audio/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_camera_transitioner/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_global_clock/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_pool/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_rpg/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_save/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_scene_transition/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_state_machine/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_toolbox/plugin.cfg")

